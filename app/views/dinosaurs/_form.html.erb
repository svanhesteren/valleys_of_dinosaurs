
<!-- <h1>Dino form!!!</h1> -->

<ul>
  <% @dinosaur.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>

<%= form_for @dinosaur do |f| %>
  <%= f.label :name%>
  <%= f.text_field :name%>

  <%= f.label :age%>
  <%= f.number_field :age%>

  <%= f.label :image_url%>
  <%= f.text_field :image_url%>

  <%= f.submit (@dinosaur.new_record? ? "Create" : "Edit")%>
<%end%>
